<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kjnresort.mapper.QnaMapper">

<insert id="register">
insert into t_qna values(seq_t_qna.nextval,#{id},#{title},#{content},sysdate,#{answer},#{answerRegDate})
</insert>

<select id="read" resultType="com.kjnresort.domain.QnaVO">
select * from t_qna where qnano=#{qnaNo}
</select>

<update id="update">
update t_qna set title=#{title},content=#{content} where qnano=#{qnaNo}
</update>

<delete id="delete">
delete t_qna where qnano=#{qnaNo} 
</delete>

<update id="updateAnswer">
update t_qna set answer=#{answer} and answerregdate=#{answerRegDate}
</update>

<update id="deleteAnswer">
update t_qna set answer='' and answerregdate='' where qnano=#{qnaNo}
</update>


